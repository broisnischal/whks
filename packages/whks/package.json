{
  "name": "whks",
  "version": "0.0.2-dev.1",
  "description": "Webhook kit for building webhook integrations",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "webhook",
    "webhooks",
    "integration",
    "discord",
    "hono"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/broisnischal/whks.git",
    "directory": "packages/whks"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "unbuild": "catalog:"
  }
}